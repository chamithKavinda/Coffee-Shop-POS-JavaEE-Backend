create database if not exists Coffee_Shop_POS;

use coffee_shop_pos;

create table Customer(
    cust_id varchar(25),
    cust_name varchar(25),
    address varchar(40),
    contact varchar(10)PRIMARY KEY
);

create table Product(
    pro_id varchar(25)PRIMARY KEY,
    pro_name varchar(25),
    price varchar(25),
    category varchar(25),
    quantity varchar(25)
);


